{
    "name": "etna/composer-behat-utils",
    "description": "ETNA Behat utils",
    "keywords": ["Behat", "utils"],
    "license": "proprietary",
    "authors": [
        {
            "name": "ETNA",
            "email": "dev@etna-alternance.net",
            "homepage": "http://etna-alternance.net"
        }
    ],
    "require": {
        "php": ">=5.5",
        "behat/behat": ">=3.2",
        "guzzlehttp/guzzle": "~6.1",
        "etna/php-rsa": "~0.2.0"
    },
    "require-dev": {
        "phpunit/php-code-coverage": "~3.0",
        "phpunit/phpcov": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "ETNA\\FeatureContext\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "repositories": [
       {
           "type": "composer",
           "url":  "https://blu-composer.herokuapp.com"
       }
   ]
}
